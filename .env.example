# Example env for Vite
VITE_API_BASE_URL=http://localhost/ohcargo/admin/api

# Optional: zone id header value used by some endpoints
VITE_ZONE_ID=

# Azure Maps Configuration
# Get your credentials at: https://portal.azure.com
VITE_AZURE_MAPS_CLIENT_ID=your_azure_maps_client_id_here
VITE_AZURE_MAPS_KEY=your_azure_maps_subscription_key_here

# Google Maps API Key (legacy - now using Azure Maps)
# Get your API key at: https://console.cloud.google.com/google/maps-apis
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
